<p id="notice"><%= notice %></p>

<h1>Encryptas</h1>

<table>
  <thead>
    <tr>
      <th>Inicial</th>
      <th>Clave</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @encryptas.each do |encrypta| %>
      <tr>
        <td><%= encrypta.inicial %></td>
        <td><%= encrypta.clave %></td>
        <td><%= link_to 'Show', encrypta %></td>
        <td><%= link_to 'Edit', edit_encrypta_path(encrypta) %></td>
        <td><%= link_to 'Destroy', encrypta, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Encrypta', new_encrypta_path %>
